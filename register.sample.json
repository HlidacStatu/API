   "searchResultTemplate": {
      "header": "",
      "body": "<!-- scriban {{ date.now }} -->\n<table class=\"table table-hover\">\n    <thead>\n        <tr>\n            <th>ČJ</th>\n            <th>Instance</th>\n            <th>Nabytí právní moci</th>\n            <th>Účastníci řízení</th>\n            <th>Věc</th>\n            <th>Typ správního řízení</th>\n        </tr>\n    </thead>\n    <tbody>\n                        \n{{ for item in model.Result }}\n           <tr>\n                <td style=\"white-space: nowrap;\">\n                    <a href=\"{{fn_DatasetItemUrl item.Id}}\">{{item.Cj}}</a>\n                </td>\n                <td style=\"white-space: nowrap;\">\n                    {{item.Instance}}\n                </td>\n                <td>\n                    {{fn_FormatDate item.PravniMoc  \"dd.MM.yyyy\"}}\n                </td>\n                <td>\n                        {{ for u in item.Ucastnici}}\n                        \n                            {{ if (fn_IsNullOrEmpty u.ICO)}}\n                            \n                                {{ fn_NormalizeText u.Jmeno }}\n                            \n                            {{else}}\n                            \n                                <a href=\"https://www.hlidacstatu.cz/subjekt/{{u.ICO}}\">{{fn_NormalizeText u.Jmeno}}</a><br />\n                            {{end #if }}\n                        {{ end #for }}\n                    \n                </td>\n                <td>\n                    {{fn_ShortenText item.Vec 200}}\n                </td>\n                <td>{{item.Typ_spravniho_rizeni}}</td>\n            </tr>\n{{end }}\n</tbody></table>",
      "footer": "",
      "title": null,
      "properties": null
   },
   "detailTemplate": {
      "header": "",
      "body": "<!-- scriban {{ date.now }} -->\n{{this.item = model}}\n<table class=\"table table-hover\">\n        <tbody>\n            <tr>\n                <td>Číslo jednací</td>\n                <td>{{item.Cj}}</td>\n            </tr>\n            <tr>\n                <td>Instance</td>\n                <td>{{item.Instance}}</td>\n            </tr>\n            <tr>\n                <td>Věc</td>\n                <td>{{item.Vec}}</td>\n            </tr>\n            <tr>\n                <td>Účastníci</td>\n                <td>\n                        {{for u in item.Ucastnici}}\n                        \n                            {{ if (fn_IsNullOrEmpty u.ICO) }}\n                            \n                                {{fn_NormalizeText u.Jmeno}}\n                            \n                            {{else}}\n                            \n                                <a href=\"https://www.hlidacstatu.cz/subjekt/{{u.ICO}}\">{{fn_NormalizeText u.Jmeno}}</a><br />\n                            {{end}}\n                        {{end}}\n                </td>\n            </tr>\n            <tr>\n                <td>Typ řízení</td>\n                <td>{{item.Typ_spravniho_rizeni}}</td>\n            </tr>\n            <tr>\n                <td>Typ rozhodnutí</td>\n                <td>{{item.Typ_spravniho_rizeni}}</td>\n            </tr>\n            <tr>\n                <td>Nabytí právní moci</td>\n                <td>{{fn_FormatDate item.PravniMoc \"dd.MM.yyyy\"}}</td>\n            </tr>\n            <tr>\n                <td>Související řízení</td>\n                <td>\n                        {{for u in item.SouvisejiciUrl}}\n                        \n                            <a href=\"{{u}}\">{{u}}</a><br />\n                        {{end}}\n\n                </td>\n            </tr>\n            <tr>\n                <td>Zdroj na UOHS</td>\n                <td><a href=\"{{item.Url}}\" target=\"_blank\">{{item.Url}}</a></td>\n            </tr>\n            {{ if item.Rozhodnuti}}\n                <tr>\n                    <td>Rozhodnutí</td>\n                    <td>\n                        <pre>\n                            {{fn_FixPlainText item.Rozhodnuti.PlainText}}\n                        </pre>\n                    </td>\n                </tr>\n            {{end}}\n        </tbody>\n    </table>",
